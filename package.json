    "react-native-share": "1.1.2",
    "react-native-svg": "7.0.2",
    "react-native-vector-icons": "5.0.0",
    "react-navigation": "2.0.1",
    "react-navigation-material-bottom-tabs": "0.1.2",
    "react-redux": "5.0.7",
    "redux": "4.0.0",
    "redux-saga": "0.16.2"
